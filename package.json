{
    "name": "engage",
    "version": "0.0.1",
    "description":
        "An educational application to help parents engage with their pre-schoolers.",
    "main": "index.js",
    "author": "hally9k@gmail.com",
    "license": "ISC",
    "repository": "git@github.com:hally9k/engage.git",
    "lint-staged": {
        "*.js": ["eslint"]
    },
    "scripts": {
        "start:local":
            "docker-compose up -d && concurrently \"nodemon index --exec babel-node --inspect\" \"webpack-dev-server\" --watch",
        "restart:local":
            "docker-compose down && docker-compose up -d && concurrently \"nodemon index --exec babel-node --inspect\" \"webpack-dev-server\" --watch",
        "start": "./node_modules/.bin/nodemon index --exec babel-node",
        "db:migrations":
            "./node_modules/.bin/knex migrate:latest --knexfile ./config/database/knexfile.js",
        "test":
            "./node_modules/.bin/mocha $(find . -name '*test.js' ! -ipath '*node_modules*')  --opts 'config/mocha/mocha.opts'",
        "e2e":
            "./node_modules/nightwatch/bin/nightwatch --config ./config/nightwatch/nightwatch.conf.js",
        "e2e:local":
            "./node_modules/nightwatch/bin/nightwatch --config ./config/nightwatch/nightwatch.conf.js -e local",
        "lint": "./node_modules/.bin/eslint app/* server/* index.js",
        "format":
            "./node_modules/.bin/prettier-eslint \"server/**/*.{js,jsx}\" \"app/**/*.{js,jsx}\" \"index.js\" --write",
        "precommit": "npm run lint && npm run type-check",
        "prepush": "npm t",
        "type-check": "./node_modules/.bin/flow"
    },
    "devDependencies": {
        "babel-eslint": "^8.0.0",
        "babel-loader": "^7.1.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "chai": "^4.1.0",
        "chai-as-promised": "^7.1.1",
        "concurrently": "^3.5.0",
        "css-loader": "^0.28.4",
        "eslint": "^4.3.0",
        "eslint-plugin-babel": "^4.1.1",
        "eslint-plugin-compat": "^1.0.4",
        "eslint-plugin-filenames": "^1.2.0",
        "eslint-plugin-flowtype": "^2.35.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.1.0",
        "file-loader": "^0.11.2",
        "flow-bin": "^0.54.1",
        "flow-typed": "^2.1.5",
        "html-webpack-plugin": "^2.30.0",
        "husky": "^0.14.3",
        "lint-staged": "^4.0.2",
        "mocha": "^3.5.0",
        "nightwatch": "^0.9.16",
        "nodemon": "^1.11.0",
        "postcss-loader": "^2.0.6",
        "prettier-eslint-cli": "^4.1.1",
        "prop-types": "^15.5.10",
        "raw-loader": "^0.5.1",
        "react-hot-loader": "^1.3.1",
        "release": "^2.4.0",
        "selenium-server": "^3.5.3",
        "sinon": "^3.3.0",
        "sinon-chai": "^2.12.0",
        "style-loader": "^0.18.2",
        "url-loader": "^0.5.9",
        "webpack": "^3.4.1",
        "webpack-dev-server": "^2.6.1"
    },
    "dependencies": {
        "babel-cli": "^6.24.1",
        "date-fns": "^1.28.5",
        "graphql": "^0.11.3",
        "graphql-request": "^1.3.3",
        "graphql-subscriptions": "^0.5.1",
        "history": "^4.7.2",
        "http": "0.0.0",
        "immutable": "^3.8.1",
        "knex": "^0.13.0",
        "koa": "^2.3.0",
        "koa-cors": "0.0.16",
        "koa-graphql": "^0.7.1",
        "koa-mount": "^3.0.0",
        "koa-static": "^4.0.1",
        "normalizr": "^3.2.3",
        "passport-local": "^1.0.0",
        "pg": "^7.0.2",
        "prettier": "^1.7.0",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-immutable-hoc": "0.0.10",
        "react-modal": "^2.2.4",
        "react-redux": "^5.0.5",
        "redis": "^2.8.0",
        "redux": "^3.7.2",
        "redux-first-router": "0.0.16-next",
        "redux-first-router-link": "^1.4.1",
        "redux-graphql-subscriptions": "0.0.9",
        "redux-immutablejs": "0.0.8",
        "redux-logger": "^3.0.6",
        "redux-observable": "^0.16.0",
        "redux-raven-middleware": "^1.2.0",
        "reselect": "^3.0.1",
        "slug": "^0.9.1",
        "subscriptions-transport-ws": "^0.9.0"
    }
}
